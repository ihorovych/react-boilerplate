{ 
  "extends": "airbnb",
  "plugins": [
    "import",
    "jsx-a11y",
    "react"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "indent": 2,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-console": "warn",
    "no-undef": "off",
    "function-paren-newline": "warn",
    "import/prefer-default-export": "off"
  }
}